doctype html 

html(lang='en')
  head
    title= title
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='stylesheet', href='/stylesheets/style.css')

  body
    div(class="bar")
      h1(class="nav-title")=title
    div(class="content")
      ul
        each item in items 
          li= item.name
    div(class="bar")